{% extends 'base.html.twig' %}

{% block body %}
    <h1>Vehicleoption</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vehicleOption.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ vehicleOption.nom }}</td>
            </tr>
            <tr>
                <th>Actif</th>
                <td>{% if vehicleOption.actif %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vehicleoption_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('vehicleoption_edit', { 'id': vehicleOption.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
