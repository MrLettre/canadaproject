<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 text-center">

        <h5>Couleurs</h5>

    </div>
</div>
</div>

<div class="row d-flex justify-content-center wow fadeIn">

<!--Grid column-->
<div class="col-xs-6 col-sm-6 col-md-6 text-center">

    {% for colors in version.couleursDisponibles %}
        <p>{{ colors.nom }}</p>
    {% endfor %}
</div>
<!--Grid column-->

<!--Grid row-->

<!--Grid column-->
<div class="col-xs-6 col-sm-6 col-md-6 text-center">

    {% for colors2 in version2.couleursDisponibles %}
        <p>{{ colors2.nom }}</p>
    {% endfor %}
</div>
<!--Grid column-->

<!--Grid row-->

</div>
