<hr>


    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 text-center">
                <tbody>
                    <tr>
                        <td>{{ vehicleDefinition.version.energie }}</td>
                        <td>Energie</td>
                        <td>{{ id2.version.energie }}</td>
                    </tr>
                </tbody>
            </div>
        </div>
    </div>
</div>

<hr>



    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 text-center">
                <tbody>
                    <tr>
                        <td>{{ vehicleDefinition.version.transmission }}</td>
                        <td>Transmission</td>
                        <td>{{ vehicle2.version.transmission }}</td>
                    </tr>
                </tbody>
            </div>
        </div>
    </div>
</div>

<hr>


<div class="container">

<div class="row">

<div class="col-xs-6 col-sm-6 col-md-6 text-center">

<table>
{% if vehicleDefinition.version.energie == 'electrique' %}
          <thead> 
            <tr>
              <th>Puissance Electrique</th>
              <th>Autonomie electrique</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>{{ vehicleDefinition.version.puissanceEl }}</td>
              <td>{{ vehicleDefinition.version.autonomieEl }}</td>
            </tr>
          </tbody>
          {% endif %}

          {% if vehicleDefinition.version.energie == 'essence' %}
          <thead> 
            <tr>
              <th>Puissance Thermique</th>
              <th>Autonomie Thermique</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>{{ vehicleDefinition.version.puissanceTh }}</td>
              <td>{{ vehicleDefinition.version.autonimieTh }}</td>
            </tr>
          </tbody>
          {% endif %}

          {% if vehicleDefinition.version.energie == 'hybride' %}
          <thead> 
            <tr>
              <th>Puissance Thermique</th>
              <th>Autonomie Thermique</th>
              <th>Puissance Electrique</th>
              <th>Autonomie electrique</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>{{ vehicleDefinition.version.puissanceTh }}</td>
              <td>{{ vehicleDefinition.version.autonimieTh }}</td>
              <td>{{ vehicleDefinition.version.puissanceEl }}</td>
              <td>{{ vehicleDefinition.version.autonomieEl }}</td>
            </tr>
          </tbody>
          {% endif %}


        </table>
</div>

<div class="col-xs-6 col-sm-6 col-md-6 text-center">

<table cellpadding="10">
{% if id2.version.energie == 'electrique' %}
          <thead> 
            <tr>
              <th>Puissance Electrique</th>
              <th>Autonomie electrique</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>{{ id2.version.puissanceEl }}</td>
              <td>{{ id2.version.autonomieEl }}</td>
            </tr>
          </tbody>
          {% endif %}

          {% if id2.version.energie == 'essence' %}
          <thead> 
            <tr>
              <th>Puissance Thermique</th>
              <th>Autonomie Thermique</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>{{ id2.version.puissanceTh }}</td>
              <td>{{ id2.version.autonimieTh }}</td>
            </tr>
          </tbody>
          {% endif %}

          {% if id2.version.energie == 'hybride' %}
          <thead> 
            <tr>
              <th>Puissance Thermique</th>
              <th>Autonomie Thermique</th>
              <th>Puissance Electrique</th>
              <th>Autonomie electrique</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td>{{ id2.version.puissanceTh }}</td>
              <td>{{ id2.version.autonimieTh }}</td>
              <td>{{ id2.version.puissanceEl }}</td>
              <td>{{ id2.version.autonomieEl }}</td>
            </tr>
          </tbody>
          {% endif %}
        </table>
</div>



</div>
</div>

<hr>

<div class="section">
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 text-center">
      <div class="table-responsive">
      <caption class="text-center">Options</caption>
        <table  class="table table-bordered table-hover">
          <tbody>
          {% for options in version.options %}
            <tr>
                <td>{{ options.nom }}</td>
              {% endfor %}
              {% for options2 in version2.options %}
                <td>{{ options2.nom }}</td>
              {% endfor %}
              </tr>
          </tbody>
          </table>
      </div><!--end of .table-responsive-->
    </div>
  </div>
</div>
</div>



<hr>


<div class="section">
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 text-center">
      <div class="table-responsive">
      <caption class="text-center">Couleurs</caption>
        <table  class="table table-bordered table-hover text-center">
          <tbody>
          {% for colors in version.couleursDisponibles %}
            <tr>
                <td>{{ colors.nom }}</td>
              {% endfor %}
              {% for colors2 in version2.couleursDisponibles %}
                <td>{{ colors2.nom }}</td>
              {% endfor %}
              </tr>
              
            
          </tbody>
          </table>
      </div><!--end of .table-responsive-->
    </div>
  </div>
</div>
