        </div>
    </div>
</div>

<div class="row d-flex justify-content-center wow fadeIn">

    <!--Grid column-->
    <div class="col-xs-6 col-sm-6 col-md-6 text-center">

        {% for options in version.options %}
            <p>{{ options.nom }}</p>
        {% endfor %}
    </div>
    <!--Grid column-->

    <!--Grid row-->

    <!--Grid column-->
    <div class="col-xs-6 col-sm-6 col-md-6 text-center">

        {% for options2 in version2.options %}
            <p>{{ options2.nom }}</p>
        {% endfor %}
    </div>
    <!--Grid column-->

    <!--Grid row-->

</div>
</div>

